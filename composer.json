{
  "name": "contributte/embedded-skeleton",
  "description": "Tech boosted project skeleton based on Nette Framework (@nette), TailwindCSS (@tailwindcss), Alpine.js (@alpinejs) and Contributte (@contributte) libraries by @f3l1x.",
  "keywords": [
    "nette",
    "skeleton",
    "project",
    "tailwindcss",
    "alpine",
    "widget",
    "embed"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "contributte/nella": "^0.1"
  },
  "require-dev": {
    "contributte/qa": "^0.2",
    "contributte/dev": "^0.3",
    "phpstan/phpstan": "^1.4.3",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "phpstan/phpstan-nette": "^1.0.0",
    "phpstan/phpstan-strict-rules": "^1.1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
